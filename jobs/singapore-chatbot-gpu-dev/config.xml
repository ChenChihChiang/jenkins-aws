    <hudson.tasks.Shell>
      <command># set image name:version
name=chatbot-dev
version=`date +&quot;%Y%m%d_%H%M&quot;`-gpu-dev

mv Dockerfile-gpu Dockerfile
mv requirements-gpu.txt requirements.txt

# build image
docker build . -t us-repo.cloud.mi.com/nuwa/$name:$version
docker tag us-repo.cloud.mi.com/nuwa/$name:$version us-repo.cloud.mi.com/nuwa/$name:latest


# push image
docker push us-repo.cloud.mi.com/nuwa/$name:$version
docker push us-repo.cloud.mi.com/nuwa/$name:latest

# /bin/ssh root@10.161.0.55 &quot;cd /var/app/docker ; docker-compose stop chatbot ; docker-compose stop chatbot-worker ; docker rmi -f cnbj-repo.cloud.mi.com/nuwa/dev-chatbot:gpu ; docker-compose up -d&quot;</command>
