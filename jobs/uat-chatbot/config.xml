    <hudson.tasks.Shell>
      <command>name=uat-chatbot
version=`date +&quot;%Y%m%d_%H%M&quot;`


# build docker image
sudo docker build . -t nuwarobotics/$name:latest

# tag docker-hub gpu, version
sudo docker tag nuwarobotics/$name:latest nuwarobotics/$name:gpu
sudo docker tag nuwarobotics/$name:latest nuwarobotics/$name:$version

# tag mi gpu, version, latest
sudo docker tag nuwarobotics/$name:latest cnbj-repo.cloud.mi.com/nuwa/$name:latest
sudo docker tag nuwarobotics/$name:latest cnbj-repo.cloud.mi.com/nuwa/$name:gpu
sudo docker tag nuwarobotics/$name:latest cnbj-repo.cloud.mi.com/nuwa/$name:$version

#push image to docker-hub, mi
sudo docker push nuwarobotics/$name:latest
sudo docker push nuwarobotics/$name:$version
sudo docker push nuwarobotics/$name:$gpu
sudo docker push cnbj-repo.cloud.mi.com/nuwa/$name:latest
sudo docker push cnbj-repo.cloud.mi.com/nuwa/$name:$version
sudo docker push cnbj-repo.cloud.mi.com/nuwa/$name:gpu


#ssh root@dev-services &quot;cd /var/app/docker/ ; docker-compose -f dev-docker.yaml pull flask&quot;
#ssh root@dev-services &quot;cd /var/app/docker/ ; docker-compose -f dev-docker.yaml stop flask&quot;
#ssh root@dev-services &quot;cd /var/app/docker/ ; docker-compose -f dev-docker.yaml rm -f flask&quot;
#ssh root@dev-services &quot;cd /var/app/docker/ ; docker-compose -f dev-docker.yaml up -d flask&quot;</command>
    </hudson.tasks.Shell>
